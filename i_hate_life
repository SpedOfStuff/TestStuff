local Player = game.Players.LocalPlayer
local JoinLogs = false

game:GetService("StarterGui"):SetCore("SendNotification", {
	Title = "donkey3donkey's Hub";
	Text = "Success! [Loaded]";
	Icon = "rbxassetid://415755273";
	Duration = 1
})

function CustomPrisonLife()
	local Player =  game.Players.LocalPlayer
	local m = Player:GetMouse()
	local MiniCooldown = false
	local Sprint = false
	local NoClip = false
	local RunService = game["Run Service"]
	RunService.RenderStepped:Connect(function()
		if NoClip == true then
			Player.Character.Humanoid:ChangeState(11)
		end
	end)

	m.KeyDown:connect(function(k)
		if k == "f" then
			if MiniCooldown == true then return end
			MiniCooldown = true
			coroutine.resume(coroutine.create(function()
				wait(0.1)
				MiniCooldown = false
			end))
			for LMFAO,Kid in pairs(game.Players:GetChildren()) do
				coroutine.resume(coroutine.create(function()
					if Kid ~= Player then for i = 1,10 do
							wait()
							game:GetService("ReplicatedStorage").meleeEvent:FireServer(unpack({Kid}))
						end
					end
				end))
			end
		end
		if k == "g" then
			if Sprint == false then
				Sprint = true
				coroutine.resume(coroutine.create(function()
					repeat
						wait(0.01)
						if Player.Character:FindFirstChildOfClass("Humanoid") ~= nil then
							Player.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = 50
						end
					until Sprint == false
					Player.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = 20
				end))
			elseif Sprint == true then
				Sprint = false
			end
		end
		if k == "h" then
			if NoClip == false then
				NoClip = true
			elseif NoClip == true then
				NoClip = false
			end
		end
	end)	
end

function MitzHub()
	local realfenv
	realfenv = hookfunction(getgenv().getfenv,function(a)
		local num = tonumber(a)
		if num then
			if num > 10 or num < 1 then
				error("Bypassing the error XD")
			else
				return realfenv()
			end
		else
			return realfenv()
		end
	end)

	local maxSim = 1000
	getgenv().setsimulationradius = newcclosure(function(i,v)
		local plr = game.Players.LocalPlayer
		sethiddenproperty(plr,"MaxSimulationRadius",maxSim)
		sethiddenproperty(plr,"SimulationRadius",maxSim)
	end)

	local sethidden
	sethidden = hookfunction(getgenv().sethiddenproperty,function(i,p,v)
		local plr = game.Players.LocalPlayer
		if i == plr then
			if (p == "MaxSimulationRadius" or p == "SimulationRadius") then
				if v == maxSim then
					sethidden(i,p,v)
				end
			else
				sethidden(i,p,v)
			end
		else
			sethidden(i,p,v)
		end
	end)
	_G.PowerfulNet = true
	_G.Jitteryness = Vector3.new(0,0,50)
	_G.CustomGui = false
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Sylixe/MiztHub/master/MainFolder/uqXAxyVdQpWTcRCdFXU6Yt2meuDkYcLUDN4SJRekQWZ5wp368zKHreYT4w82AH4U.lua",true))()
end

function Nullware()
	getgenv().Nullware_ReanimateConfiguration = {
		["Netless"] = true, --Toggles Netless.
		["Anti-Fling"] = false, --Toggles Anti-Fling.
		["Hats To Align"] = {}, --List of hats that you want to align. (e.g. {"Hat1", "Hat2"} or {"All"})
		["Head Movement Without Godmode"] = false, --Toggles Head Movement Without Godmode.
		["Enable Limb Collisions"] = false, --Enables your limb's collisions. (Overrides "Disable Torso Collisions" if enabled, also doesn't require godmode)
		["Disable Torso Collisions"] = false, --Disables your torso's collisions. (Doesn't work with godmode)
		["R15 To R6"] = false, --Toggles R15 To R6.
		["Godmode"] = false --Toggles Godmode.
	}
	loadstring(game:HttpGetAsync("https://gist.githubusercontent.com/M6HqVBcddw2qaN4s/8cd532018277ee9982433eea25a6c14f/raw/sewMjTpJvVBLR96L"))()
end

function SeptexPrisonLife()
	loadstring(game:HttpGet(('https://raw.githubusercontent.com/XTheMasterX/Scripts/Main/PrisonLife'),true))()
end

game.Players.PlayerAdded:Connect(function(p)
	if JoinLogs == true then
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack({[1] = "+ "..p.Name.." +",[2] = "All",}))
	end
end)
game.Players.PlayerRemoving:Connect(function(p)
	if JoinLogs == true then
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack({[1] = "- "..p.Name.." -",[2] = "All",}))
	end
end)

Player.Chatted:connect(function(msg)
	if msg == ";register" then
		if game.PlaceId == 155615604 then
			SeptexPrisonLife()
		end
	elseif msg == ";reanimation" then
		MitzHub()
	elseif msg == ";custom" then
		if game.PlaceId == 155615604 then
			CustomPrisonLife()
		end	
	elseif msg == ";logs on" then
		JoinLogs = true
	elseif msg == ";logs off" then
		JoinLogs = false
	elseif msg:sub(1,6) == ";goto " then
		local PlayerTargetSub = msg:sub(7)
		local Target = nil
		for i,v in pairs(game.Players:GetChildren()) do if v.Name:sub(1,string.len(PlayerTargetSub)) == PlayerTargetSub then Target = v end end
		if Target ~= nil then Player.Character.HumanoidRootPart.CFrame = Target.Character.HumanoidRootPart.CFrame * CFrame.new(0,5,0) end
	elseif msg == ";null" then
		Nullware()
	end
end)

