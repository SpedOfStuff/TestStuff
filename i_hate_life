local Player = game.Players.LocalPlayer
local m = Player:GetMouse()
local MiniCooldown = false
local Sprint = false
local NoClip = false
local JoinLogs = false
local CurrentGame = "NIL"
local JLogs = {}
local Position = nil
local SpawnPosition = nil
local ToggledSpawn = false

if game.PlaceId == 155615604 then
game:GetService("StarterGui"):SetCore("SendNotification", { Title = "DonkeyGui"; Text = "'Prison Life' Detected!"; Icon = "rbxassetid://607866915"; Duration = 1 }) CurrentGame = "PRISONLIFE" elseif game.PlaceId == 183079390 then
game:GetService("StarterGui"):SetCore("SendNotification", { Title = "DonkeyGui"; Text = "'Sheesh!' Detected!"; Icon = "rbxassetid://607866915"; Duration = 1 }) CurrentGame = "SHEESH" else
game:GetService("StarterGui"):SetCore("SendNotification", { Title = "DonkeyGui"; Text = "Unsupported Game"; Icon = "rbxassetid://1130614771"; Duration = 1 }) CurrentGame = "NONE" end
wait(0.25)
if Player.Character:FindFirstChildOfClass"Humanoid".RigType == Enum.HumanoidRigType.R6 then
game:GetService("StarterGui"):SetCore("SendNotification", { Title = "Rig Type"; Text = "R6"; Duration = 0.5 }) else
game:GetService("StarterGui"):SetCore("SendNotification", { Title = "Rig Type"; Text = "R15"; Duration = 0.5 }) end

local RunService = game["Run Service"]
RunService.RenderStepped:Connect(function()
if NoClip == true then Player.Character.Humanoid:ChangeState(11) end end)

function CustomPrisonLife()
	m.KeyDown:connect(function(k)
		if k == "f" then
			if MiniCooldown == true then return end
			MiniCooldown = true
			coroutine.resume(coroutine.create(function()
				wait(0.1)
				MiniCooldown = false
			end))
			for LMFAO,Kid in pairs(game.Players:GetChildren()) do
				coroutine.resume(coroutine.create(function()
					if Kid ~= Player then for i = 1,10 do
							wait()
							game:GetService("ReplicatedStorage").meleeEvent:FireServer(unpack({Kid}))
						end
					end
				end))
			end
		end
		if k == "g" then
			if Sprint == false then
				Sprint = true
				coroutine.resume(coroutine.create(function()
					repeat
						wait(0.01)
						if Player.Character:FindFirstChildOfClass("Humanoid") ~= nil then
							Player.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = 50
						end
					until Sprint == false
					Player.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = 20
				end))
			elseif Sprint == true then
				Sprint = false
			end
		end
		if k == "h" then
			if NoClip == false then
				NoClip = true
			elseif NoClip == true then
				NoClip = false
			end
		end
	end)	
end

function MitzHub()
	local realfenv
	realfenv = hookfunction(getgenv().getfenv,function(a) local num = tonumber(a) if num then if num > 10 or num < 1 then error("Bypassing the error XD") else return realfenv() end else return realfenv() end end)
	local maxSim = 1000
	getgenv().setsimulationradius = newcclosure(function(i,v) local plr = game.Players.LocalPlayer sethiddenproperty(plr,"MaxSimulationRadius",maxSim) sethiddenproperty(plr,"SimulationRadius",maxSim) end)
	local sethidden
	sethidden = hookfunction(getgenv().sethiddenproperty,function(i,p,v) local plr = game.Players.LocalPlayer if i == plr then if (p == "MaxSimulationRadius" or p == "SimulationRadius") then if v == maxSim then sethidden(i,p,v) end else sethidden(i,p,v) end else sethidden(i,p,v) end end)
	_G.PowerfulNet = true
	_G.Jitteryness = Vector3.new(0,0,50)
	_G.CustomGui = false
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Sylixe/MiztHub/master/MainFolder/uqXAxyVdQpWTcRCdFXU6Yt2meuDkYcLUDN4SJRekQWZ5wp368zKHreYT4w82AH4U.lua",true))()
end

function Nullware() getgenv().Nullware_ReanimateConfiguration = {["Netless"] = true,["Anti-Fling"] = false,["Hats To Align"] = {},["Head Movement Without Godmode"] = false,["Enable Limb Collisions"] = false,["Disable Torso Collisions"] = false,["R15 To R6"] = false,["Godmode"] = false} loadstring(game:HttpGetAsync("https://gist.githubusercontent.com/M6HqVBcddw2qaN4s/8cd532018277ee9982433eea25a6c14f/raw/sewMjTpJvVBLR96L"))() end

function SeptexPrisonLife() loadstring(game:HttpGet(('https://raw.githubusercontent.com/XTheMasterX/Scripts/Main/PrisonLife'),true))() end

Player.CharacterAdded:connect(function(c) if ToggledSpawn == false then return end c.HumanoidRootPart.CFrame = SpawnPosition end)
game.Players.PlayerAdded:Connect(function(p) if JoinLogs == true then game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Joined";Text = p.Name;Icon = "rbxassetid://521627456";Duration = 0.5}) JLogs[#JLogs + 1] = p.Name.."Joined" end end)
game.Players.PlayerRemoving:Connect(function(p) if JoinLogs == true then game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Left";Text = p.Name;Icon = "rbxassetid://17426572";Duration = 0.5}) JLogs[#JLogs + 1] = p.Name.."Left" end end)

Player.Chatted:connect(function(msg)
	if CurrentGame == "PRISONLIFE" then
		if msg:lower() == ";classic" then
			SeptexPrisonLife()
		elseif msg:lower() == ";custom" then
			CustomPrisonLife()
		end
	end
	if msg == ";reanimation" then
		MitzHub()
		game:GetService("StarterGui"):SetCore("SendNotification", {Title = "MitzHub!"; Text = "Loading, Expect Lag And Delay."; Icon = "rbxassetid://35365756"; Duration = 2 })
	elseif msg == ";logs on" then
		JoinLogs = true
		game:GetService("StarterGui"):SetCore("SendNotification", { Title = "Logs"; Text = "Logging"; Icon = "rbxassetid://5396216886"; Duration = 2 })
	elseif msg == ";logs off" then
		JoinLogs = false
		game:GetService("StarterGui"):SetCore("SendNotification", { Title = "Logs"; Text = "Not Logging"; Icon = "rbxassetid://5396216886"; Duration = 2 })
	elseif msg == ";readlogs" then
		for i,v in pairs(JLogs) do print(v) end
	elseif msg == ";clearlogs" then
		JLogs = {}
	elseif msg:sub(1,6) == ";goto " then
		local PlayerTargetSub = msg:sub(7)
		local Target = nil
		for i,v in pairs(game.Players:GetChildren()) do if v.Name:sub(1,string.len(PlayerTargetSub)) == PlayerTargetSub then Target = v end end
		if Target ~= nil then Player.Character.HumanoidRootPart.CFrame = Target.Character.HumanoidRootPart.CFrame end
	elseif msg == ";save" then
		Position = Player.Character.HumanoidRootPart.CFrame
	elseif msg == ";load" then
		Player.Character.HumanoidRootPart.CFrame = Position
	elseif msg == ";spawn" then
		SpawnPosition = Player.Character.HumanoidRootPart.CFrame
		ToggledSpawn = true
	end
end)
